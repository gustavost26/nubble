{
  "name": "nubble",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "pods-deintegrate": "cd ios && bundle exec pod deintegrate",
    "pods-update": "cd ios && bundle exec pod install --repo-update",
    "pod-install": "cd ios && pod install",
    "rm-derivedData": "rm -rf ~/Library/Developer/Xcode/DerivedData",
    "rm-pods": "cd ios && rm -rf Pods Podfile.lock build && yarn rm-derivedData",
    "open": "open ios/nubble.xcworkspace/",
    "open2": "cd ios && xed ."
  },
  "dependencies": {
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/native-stack": "^7.3.13",
    "@shopify/restyle": "^2.4.5",
    "react": "19.0.0",
    "react-native": "0.79.0",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0",
    "react-native-svg": "^15.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.0",
    "@react-native/eslint-config": "0.79.0",
    "@react-native/metro-config": "0.79.0",
    "@react-native/typescript-config": "0.79.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "build",
      "ci",
      "docs",
      "feat",
      "fix",
      "perf",
      "refactor",
      "style",
      "test",
      "revert",
      "chore"
    ],
    "scope": "\\S+.*"
  },
  "engines": {
    "node": ">=18"
  }
}

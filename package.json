{
	"name": "nubble",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"lint": "eslint .",
		"start": "react-native start",
		"test": "jest",
		"pods-deintegrate": "cd ios && bundle exec pod deintegrate",
		"pods-update": "cd ios && bundle exec pod install --repo-update",
		"rm-derivedData": "rm -rf ~/Library/Developer/Xcode/DerivedData",
		"rm-pods": "cd ios && rm -rf Pods Podfile.lock build && yarn rm-derivedData"
	},
	"dependencies": {
		"@shopify/restyle": "^2.4.5",
		"react": "18.2.0",
		"react-native": "0.73.6"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@babel/preset-env": "^7.20.0",
		"@babel/runtime": "^7.20.0",
		"@react-native/babel-preset": "0.73.21",
		"@react-native/eslint-config": "0.73.2",
		"@react-native/metro-config": "0.73.5",
		"@react-native/typescript-config": "0.73.1",
		"@types/react": "^18.2.6",
		"@types/react-test-renderer": "^18.0.0",
		"babel-jest": "^29.6.3",
		"eslint": "^8.19.0",
		"jest": "^29.6.3",
		"prettier": "2.8.8",
		"react-test-renderer": "18.2.0",
		"typescript": "5.0.4"
	},
	"commitplease": {
		"style": "angular",
		"types": [
			"build",
			"ci",
			"docs",
			"feat",
			"fix",
			"perf",
			"refactor",
			"style",
			"test",
			"revert"
		],
		"scope": "\\S+.*"
	},
	"engines": {
		"node": ">=18"
	}
}
